<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="kosta.project.three.model.RvDAO">
 
 	<select id="getLocList" resultType="kosta.project.three.model.LocDTO">
 		SELECT * FROM LOCATION
 	</select>
 	<select id="getLocName" parameterType="String" resultType="String">
 		SELECT LOC_NAME FROM LOCATION WHERE LOC_CODE= #{LOC_CODE}
 	</select>
 	
 
 </mapper>